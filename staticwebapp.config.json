{
  "trailingSlash": "never",
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=7776000"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    },
    {
      "route": "/doc/(.*)",
      "rewrite": "https://doc.metalama.net/$1"
    }
  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, OPTIONS"
  }
}